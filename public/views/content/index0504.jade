extends layout

block content
  //div(id='dimScreenFull',ng-hide="global.get('user').val")
  div(id='dimScreen',content-loaded)
    img(src='views/clinic/img/spinner.gif')
  div(id="o-wrapper" class="o-wrapper",ng-cloak,ng-init="displaySlideMenu=true;setInitData(#{JSON.stringify(store)},#{JSON.stringify(mobile)})")
    toaster-container
    div(class="navbar navbar-fixed-top" id='topMenu')
        div(class="pull-left left-menu")
          ul
            li
              a(ui-sref="frame",title="весь контент")
                span
                  i(class="material-icons") homes
            li
              a(ng-click="slideMenu=true",title="открыть каталог")
                span
                  i(class="material-icons") menu
            li
              a(ui-sref="frame.help",title="помощь")
                span
                  i(class="material-icons") info_outline
              a( ng-if="helper.intro",ng-click="getIntro()",title="помощь")
                span
                  i(class="material-icons") info_outline
        div( class="seller pull-left", ng-if="!global.get.mobile")
          a(ng-click="logout()")
            span(ng-if="global.get('seller').val")
                span(ng-bind="global.get('user').val.name" )
          span(class="location") управление контентом
        div(class="dropdown pull-right right-menu navbar-menu")
          a(class='dropdown-toggle',data-target="#",data-toggle="dropdown")
            i(class="material-icons menu-icon") apps
          ul(class="dropdown-menu")
            li
              a(ng-click="changeLocation('/setting')")
                i(class="material-icons link-warning") settings_applications
                | настройки магазина
            li
              a(ng-click="changeLocation('/manage')")
                i(class="material-icons link-warning") shopping_basket
                | управление  заказами
            li
              a(ng-click="changeLocation('/promo')")
                i(class="material-icons link-warning") dashboard
                | управление промо
            li
              a(ng-click="changeLocation('/seo')")
                i(class="material-icons link-warning") trending_up
                | управление SEO
            li
              a(href='/home',target="blank")
                i(class="material-icons link-warning") airplay
                | перейти на сайт
        div(class="pull-right right-menu")
          a
            span(class="for-saving save",ng-class="{'saving':global.get('saving').val}")
              | сохранено
          a(class="btn btn-fab btn-fab-mini btn-lang",ng-repeat="lang in global.get('store').val.langArr",ng-click="changeLang(lang)")
            span(ng-class="{'active':lang==global.get('store').val.lang}",ng-bind="lang")



        //a(ui-sref="frame",class="navbar-brand")
          i(class="material-icons") homes
      //div(ng-if="$state.current.name=='frame.stuffs'",menu-sections)


    div(class="admin-menu",pageslide,ps-open="slideMenu",ps-side='left',ps-speed="0.3",ps-size='400px',click-outside="global.get('functions').val.closeMenu()",id='slideMenu')
      div(class="vertical-menu")
        a(ng-click="slideMenu=false",class="button cancel")
          //img(class="close",src="views/template/img/icon/clear.png")
        div(class="menu-section-vertical")
          slide-menu(display-slide-menu="slideMenu")
          //h1 sdfsdfs
          //h2 sdfsdfsdf
          //h3 sdfsdfsdf
    div()
      //div(style="position:absolute;top:0,left:0;width:300px;height:100%")
        h3 dd
      div(ui-view)
