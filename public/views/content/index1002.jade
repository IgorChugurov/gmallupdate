extends layout

block content
  div(id="o-wrapper" class="o-wrapper",ng-cloak,ng-init="displaySlideMenu=false;setInitData(#{JSON.stringify(store)},#{JSON.stringify(user)},#{JSON.stringify(admin)},#{JSON.stringify(mobile)},#{JSON.stringify(seller)})")

    toaster-container
    //div(style="z-index:1301;position:fixed;top:0px", left-slide-menu)
      a(ng-click="displaySlideMenu=!displaySlideMenu;",class="navbar-brand")
        i(class="material-icons") reorder
    //div(style="z-index:1301;position:fixed;top:0px;left:50px")
      a(ng-click="displaySlideMenu=!displaySlideMenu;",class="navbar-brand") Tatiana
    //.right-slide-menu
      a(ng-click="displaySlideMenu=!displaySlideMenu;",class="navbar-brand") Tatiana

    div(class="navbar navbar-default navbar-fixed-top" id='topMenu')
