extends layout

block content
  div(id='dimScreenFull',ng-hide="global.get('user').val")
  div(id="o-wrapper" class="o-wrapper",ng-cloak,ng-init=ngInit)
    toaster-container
    div(class="navbar navbar-default navbar-fixed-top" id='topMenu')
      .container-fluid
        button(type="button",class="navbar-toggle",data-toggle="collapse",data-target=".navbar-responsive-collapse")
          span(class="icon-bar")
          span(class="icon-bar")
          span(class="icon-bar")
        a(ui-sref="frame",class="navbar-brand")
          i(class="material-icons") homes

        a(ui-sref="frame",class="navbar-brand")
          span(ng-if="global.get('seller').val") продавец&nbsp
          span(ng-bind="global.get('user').val.name")



        //a(ui-sref="frame",class="navbar-brand")
          i(class="material-icons") homes
        div(class="dropdown pull-right navbar-brand")
          a(class='dropdown-toggle',data-target="#",data-toggle="dropdown",style="color:#fff;")
            i(class="material-icons") apps

          ul(class="dropdown-menu")
            li
              a(ng-click="changeLocation('/manage')")
                i(class="material-icons link-warning") shopping_basket
                | управление  заказами
            li
              a(ng-click="changeLocation('/setting')")
                i(class="material-icons link-warning") shopping_basket
                | настройки магазина
            li
              a(ng-click="changeLocation('/content')")
                i(class="material-icons link-warning") description
                | управление контентом
            li
              a(ng-click="changeLocation('/promo')")
                i(class="material-icons link-warning") dashboard
                | управление промо
            li
              a(ng-click="changeLocation('/seo')")
                i(class="material-icons link-warning") trending_up
                | управление продвижение SEO
            li
              a(ng-click="changeLocation('/home')")
                i(class="material-icons link-warning") homes
                | перейти на сайт
        a(class='pull-right navbar-brand')
          i(class="material-icons") info_outline


    div(style="margin-top:70px")
      //div(style="position:absolute;top:0,left:0;width:300px;height:100%")
        h3 dd
      div(ui-view)
