            if(sn.pin && sn.pin.is)
                li
                    a(class="sn",href=sn.pin.link,target="_blank")
                        span(set-animate=dj[key].animate,data-animation=dj[key].animate) Pinterest

            if(sn.tw && sn.tw.is)
                li
                    a(class="sn",href=sn.tw.link,target="_blank")
                        span(set-animate=dj[key].animate,data-animation=dj[key].animate) Twitter
            if(sn.vk && sn.vk.is)
                li
                    a(class="sn",href=sn.vk.link,target="_blank")
