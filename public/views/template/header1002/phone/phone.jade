li(class="nav-item")
    div(class=dj[key].wrapclass+pullClass)
        div(class=mobileWrapper)
            if(dj[key].positionMenu=='top' ||dj[key].positionMenu=='bottom')
                - var c = 'page-stat sale nav-group'
            else
                - var c = 'page-stat sale'
            div(class=c)
                each phone in dj[key].phone
                    a(href='tel:'+phone)
                        span(class="part")=phone
            div(class="clearfix")