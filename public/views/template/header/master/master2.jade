div(class=dj[key].wrapclass+pullClass,set-animate=dj[key].blockAnimate,data-animation=dj[key].blockAnimate,data-animation-delay=dj[key].animationDelay)
    div(class=mobileWrapper)
        if(dj[key].positionMenu=='center' ||dj[key].positionMenu=='bottom')
            - var c = 'page-stat nav-group master'
        else
            - var c = 'page-stat master'
        div(class=c,ng-if="global.get('masters').val.length")
            a(href="/master"+dj[key].addLink,set-animate=dj[key].animate,data-animation=dj[key].animate,ng-class="{'active':$state.current.name=='master'||$state.current.name=='master.item'}")
                if(store.nameLists.masterList)
                    span(class="part")=store.nameLists.masterList
                else
                    span(class="part")=lang.master


