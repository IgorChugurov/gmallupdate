div(class=dj[key].wrapclass+pullClass)
    div(class=mobileWrapper)
        - var c = 'logo-top'
        div(class=c)
            a(href="/home")
                - var src = photoHost+"/"+store.logo
                - var srcInv = photoHost+"/"+store.logoInverse
                if(store.logo)
                    img(id="mainLogo",src=src,class="img-logo",alt="logo",add-class-by-scroll='logo-out')
                if(store.logoInverse)
                    img(id="inverseLogo",src=srcInv,class="img-logoInv",alt="logo",add-class-by-scroll='logo-out')

