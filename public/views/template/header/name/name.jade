div(class=dj[key].wrapclass+pullClass)
    div(class=mobileWrapper)
        if(dj[key].positionMenu=='top' ||dj[key].positionMenu=='bottom')
            - var c = 'page-stat name-box nav-group'
        else
            - var c = 'page-stat name'
        div(class=c)
            a(href="/home")
                span(class="part name",set-animate=dj[key].animate,data-animation=dj[key].animate)=store.name
