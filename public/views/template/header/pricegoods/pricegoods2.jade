div(class=dj[key].wrapclass+pullClass,set-animate=dj[key].blockAnimate,data-animation=dj[key].blockAnimate,data-animation-delay=dj[key].animationDelay)
    div(class=mobileWrapper)
        if(dj[key].positionMenu=='top' ||dj[key].positionMenu=='bottom')
            - var c = 'page-stat new nav-group'
        else
            - var c = 'page-stat new'
        div(class=c)
            a(href="/pricegoods",set-animate=dj[key].animate,data-animation=dj[key].animate)
                if(store.nameLists.pricegoods)
                    span(class="part")=store.nameLists.pricegoods
                else
                    span(class="part")=lang.pricegoods
        div(class="clearfix")