div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 info-block")
    div(class="col-lg-3 col-md-3 col-sm-12 col-xs-12 menu-left")
        each item in info
            a(href='/info/'+item.url)
                h2(class=(item.active)?'active':'')=item.name
    div(class="col-lg-9 col-md-9 col-sm-12 col-xs-12")

            uib-accordion(close-others="oneAtATime")
                div(uib-accordion-group,class="panel-default",heading="sssss")
                    p This is just some content to illustrate fancy headings.
                - for(var i =0; i< dj[key].itemDetail.blocks.length; i++)
                    -var block=dj[key].itemDetail.blocks[i];block.open=true
                    div(uib-accordion-group,class="panel-default",heading="sssss")
                        p This is just some content to illustrate fancy headings.
    div(class="clearfix")
div(class="clearfix")