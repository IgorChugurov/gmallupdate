div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="stuff-list-wrapper",id="items-list-wrapper")
            div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 price-box")
                div(class="container")
                    h1=dj[key].listName
                    table(class="table table-striped table-bordered")
                            each category,idCat in dj[key].categories
                                thead
                                    tr()
                                        td(scope="row",colspan="3",class="category-name")
                                            h2=category.name
                                    tr
                                        th()
                                            span=lang.nomination

                                        th()
                                            span=lang.duration

                                        th()
                                            span=lang.cost
                                tbody
                                each item,$index in category.stuffs
                                    if(item.sortsOfStuff && item.sortsOfStuff.differentPrice)
                                        each sort,tag in item.stock
                                            tr
                                                td(scope="row")
                                                    p
                                                        a(href=item.link)
                                                            span=item.name
                                                            if(dj[key].filterTags[tag])
                                                                span &nbsp
                                                                span=dj[key].filterTags[tag]
                                                    if(item.masters)
                                                        p
                                                            each m in item.masters
                                                                a(class="master-name",href='/master/'+m.url)
                                                                    span=m.name+' '

                                                td()
                                                    span=item.timePart * 15
                                                    span &nbsp;
                                                    span=lang.minute


                                                td()
                                                    span=lang.from
                                                    span &nbsp;
                                                    if(sort.price.toFixed)
                                                        span=sort.price.toFixed(dj[key].formatPrice)
                                                    else
                                                        span=Number(sort.price).toFixed(dj[key].formatPrice)
                                                    span &nbsp;
                                                    span=dj[key].currency
                                    else
                                        tr
                                            td(scope="row")
                                                p
                                                    a(href=item.link)
                                                        span=item.name
                                                if(item.masters)
                                                    p
                                                        each m in item.masters
                                                            a(class="master-name",href='/master/' + m.url)
                                                                span=m.name + ' '

                                            td()
                                                span=item.timePart * 15
                                                span &nbsp;
                                                span=lang.minute


                                            td()
                                                span=lang.from
                                                span &nbsp;
                                                if(item.price)
                                                    span=Number(item.price).toFixed(dj[key].formatPrice)
                                                else
                                                    span='0.00'
                                                span &nbsp;
                                                span=dj[key].currency
                if(titles.desc)
        div(class="seo-desc col-lg-12 col-md-12 col-sm-12 col-xs-12 ")
            |!{titles.desc}
div(class="clearfix")