div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="stuff-list-wrapper",id="items-list-wrapper")
            div(class="container")
                div()
                    h1=dj[key].listName
                    each item,$index in dj[key].items
                        div(class="col-lg-8 col-md-8 col-sm-8 col-xs-8")
                            p
                                a(href=item.link)
                                    span=item.name
                                    if(item.timePart)
                                        span &nbsp;(
                                        span=item.timePart * 15
                                        span &nbsp;
                                        span=lang.minute
                                        span )
                        div(class="col-lg-4 col-md-4 col-sm-4 col-xs-4")
                            p=item.price
        div(class="clearfix")
    if(titles.desc)
        div(class="seo-desc col-lg-12 col-md-12 col-sm-12 col-xs-12 ")
            |!{titles.desc}
div(class="clearfix")