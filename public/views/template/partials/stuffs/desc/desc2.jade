div(class=dj[key].wrapclass,class="col-lg-12 col-md-12 col-sm-12 col-xs-12")
    div(class=mobileWrapper)
        if(dj[key].banner)
            img(class="img-responsive",src=dj[key].banner)
        div(class="page-desc-box")
            div(class="container")
                div(class="desc-box")
                    h1(class="page-name")=dj[key].name
                    if(dj[key].desc)
                        div(class="desription-page")
                            | !{titles.desc}





