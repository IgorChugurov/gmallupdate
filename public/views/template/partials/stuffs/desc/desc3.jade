div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        if(dj[key].banner)
            img(class="img-responsive",src=dj[key].banner)
        div(class="container")
            div(class="desc-content")
                if(dj[key].name)
                    h1(class="page-name")=dj[key].name
                if(dj[key].desc)
                    div(class="desription-page")
                        | !{dj[key].desc}
div(class="clearfix")


