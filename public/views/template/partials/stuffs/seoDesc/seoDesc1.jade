div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        if(titles.desc)
            div(class="container")
                div(class="seo-desc")
                    h3(ng-click="isCollapsed = !isCollapsed")
                        span(class="pull-left",ng-bind="$ctrl.global.get('category').val.name")
                        span(ng-show="isCollapsed",class="pull-right")
                            span(class="icon-delete-img")
                        
                        span(ng-show="!isCollapsed",class="pull-right")
                            span(class="icon-plus-img")
                        div(class="clearfix")
                    div(uib-collapse="!isCollapsed")
                        | !{titles.desc}
        div( class="clearfix")

    