div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(ng-switch="$ctrl.data.rows",class="stuff-list-wrapper")
            - var classList = "col-lg-12 col-md-12 col-sm-12 col-xs-12";
            - var classList12 = "col-lg-12 col-md-12 col-sm-12 col-xs-12";
            - var classList9="col-lg-9 col-md-9 col-sm-9 col-xs-9"
            if(filtersInList)
                - classList="col-lg-9 col-md-9 col-sm-8 col-xs-8"

                div(class="filters-wrap col-lg-3 col-md-3 col-sm-4 col-xs-4",ng-show="$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll")
            div(ng-switch-when="1",ng-class="{'col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box':$ctrl.$stateParams.categoryUrl=='category' && !$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll,'"+classList+"':$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll}")
                empty-list(ng-show="$ctrl.items && !$ctrl.items.length")
                div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.items",stuff-cart)

            div(ng-switch-when="2",ng-class="{'col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box':$ctrl.$stateParams.categoryUrl=='category' && !$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll,'"+classList+"':$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll}")
                empty-list(ng-show="$ctrl.items && !$ctrl.items.length")
                div(class="col-md-6 col-sm-6 col-sm-6 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr2[0]", stuff-cart)

                div(class="col-md-6 col-sm-6 col-sm-6 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr2[1]", stuff-cart)

            div(ng-switch-when="3" ,ng-class="{'col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box':$ctrl.$stateParams.categoryUrl=='category' && !$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll,'"+classList+"':$ctrl.$stateParams.categoryUrl!='category' ||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll}")
                empty-list(ng-show="$ctrl.items && !$ctrl.items.length")
                div(class="col-md-4 col-sm-4 col-sm-4 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr3[0]", stuff-cart)

                div(class="col-md-4 col-sm-4 col-sm-4 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr3[1]", stuff-cart)

                div(class="col-md-4 col-sm-4 col-sm-4 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr3[2]", stuff-cart)

            div(ng-switch-when="4",ng-class="{'col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box':$ctrl.$stateParams.categoryUrl=='category' && !$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll,'"+classList+"':$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll}")
                empty-list(ng-show="$ctrl.items && !$ctrl.items.length")
                //div(class="",ng-if="!$ctrl.items.length && $ctrl.stateComplite")
                    img(class="img-responsive",ng-src="{{$ctrl.global.get('store').val.emptyList}}")
                div(class="col-md-3 col-sm-3 col-sm-3 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr4[0]", stuff-cart)

                div(class="col-md-3 col-sm-3 col-sm-3 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr4[1]", stuff-cart)

                div(class="col-md-3 col-sm-3 col-sm-3 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr4[2]", stuff-cart)

                div(class="col-md-3 col-sm-3 col-sm-3 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr4[3]", stuff-cart)
            div(ng-switch-when="5",ng-class="{'col-lg-12 col-md-12 col-sm-12 col-xs-12 list-box':$ctrl.$stateParams.categoryUrl=='category' && !$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll,'"+classList+"':$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll}")
                empty-list(ng-show="$ctrl.items && !$ctrl.items.length")
                //div(class="",ng-if="!$ctrl.items.length && $ctrl.stateComplite")
                    img(class="img-responsive",ng-src="{{$ctrl.global.get('store').val.emptyList}}")
                div(class="col-md-2 col-sm-2 col-sm-2 col-xs-12 col-md-offset-1 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr5[0]", stuff-cart)
                div(class="col-md-2 col-sm-2 col-sm-2 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr5[1]", stuff-cart)
                div(class="col-md-2 col-sm-2 col-sm-2 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr5[2]", stuff-cart)
                div(class="col-md-2 col-sm-2 col-sm-2 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr5[3]", stuff-cart)
                div(class="col-md-2 col-sm-2 col-sm-2 col-xs-12 list-box")
                    div(class="row")
                        div(ng-repeat="stuff in $ctrl.itemsArr5[4]", stuff-cart)
            div(class="clearfix")

paginator-main(paginate="$ctrl.paginate",getlist="$ctrl.getList()",scroll="{{scrollId}}" )
div(class="seo-desc col-lg-12 col-md-12 col-sm-12 col-xs-12 ",ng-bind-html="$ctrl.global.get('titles').val.desc|unsafe")
div(class="clearfix")