div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="stuff-list-wrapper container",id="items-list-wrapper")
            if(dj[key].filtersInlist)
                div(class="filters-wrap col-lg-3 col-md-3 col-sm-3 col-xs-3 td-list",ng-show="$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll")
            div(class='list-class '+dj[key].classList)
                div(id='td-list-1',class="col-lg-12 col-md-12 col-sm-12 col-xs-12 td-list")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].items
                            div(class="td-inner",stuff-in-list=stuff.stuffInList)
                                div
        div(class="clearfix")
        div(id="paginateData"+dj[key].page,data-total=dj[key].total,data-qty=dj[key].qty,data-page=dj[key].page,data-last-item-id=dj[key].lastItemId)
div(class="clearfix")