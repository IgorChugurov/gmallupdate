div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="stuff-list-wrapper container",id="items-list-wrapper")
            if(dj[key].filtersInlist)
                div(class="filters-wrap td-list "+dj[key].filtersClass,ng-show="$ctrl.$stateParams.categoryUrl!='category'||$ctrl.global.get('store').val.template.stuffListType[$ctrl.global.get('sectionType').val].filtersForAll")
            div(class='list-class '+dj[key].classList)
                div(id='td-list-1',class="col-md-4 col-md-4 col-sm-4 col-xs-4 td-list")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].itemsArr3[0]
                            div(class="td-inner",stuff-in-list=stuff.stuffInList,group-stuffs-in-list=stuff.groupStuffsInList)
                                div
                div(id='td-list-2',class="col-md-4 col-md-4 col-sm-4 col-xs-4 td-list")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].itemsArr3[1]
                            div(class="td-inner",stuff-in-list=stuff.stuffInList,group-stuffs-in-list=stuff.groupStuffsInList)
                                div
                div(id='td-list-3',class="col-md-4 col-md-4 col-sm-4 col-xs-4 td-list")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].itemsArr3[2]
                            div(class="td-inner",stuff-in-list=stuff.stuffInList,group-stuffs-in-list=stuff.groupStuffsInList)
                                div
        div(class="clearfix")
        div(id="paginateData"+dj[key].page,data-total=dj[key].total,data-qty=dj[key].qty,data-page=dj[key].page,data-last-item-id=dj[key].lastItemId)
div(class="clearfix")