div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="stuff-list-wrapper container",id="items-list-wrapper")
            if(dj[key].filtersInlist)
                div(class="filters-wrap td-list "+dj[key].filtersClass,ng-show="$ctrl.displayableFilters")
            div(class='list-class '+dj[key].classList)
                div(id='td-list-1',class="col-lg-6 col-md-6 col-sm-6 col-xs-6 td-list list-one")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].itemsArr2[0]
                            div(class="td-inner",stuff-in-list=stuff.stuffInList)
                                div
                div(id='td-list-2',class="col-lg-6 col-md-6 col-sm-6 col-xs-6 td-list list-two")
                    div(class="td-wrapper")
                        each stuff,$index in dj[key].itemsArr2[1]
                            div(class="td-inner",stuff-in-list=stuff.stuffInList)
                                div
        div(class="clearfix")
        div(id="paginateData"+dj[key].page,data-total=dj[key].total,data-qty=dj[key].qty,data-page=dj[key].page,data-last-item-id=dj[key].lastItemId)
div(class="clearfix")