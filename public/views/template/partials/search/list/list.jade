div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12")
            div(class="container")
                h2(class="text-center header-page")=dj[key].modelName
                each item in dj[key].items
                    div(class="search-list-box")
                        div(class="col-lg-2 col-md-2 col-sm-4 col-xs-4 search-list-box-img")
                            if(item.img)
                                a(href='/' + dj[key].model + '/' + item.url)
                                    img(class="img-responsive banner-static",src=photoHost + '/' + item.img)
                                    p(class="see text-center")
                                        span(class="icon-see-img")
                            else
                                if(item.video)
                                    div(class="search-list-video")
                                        | !{item.video}

                        div(class="col-lg-10 col-md-10 col-sm-8 col-xs-8")
                            a(href='/'+dj[key].model+'/'+item.url)
                                h3(class="search-header")
                                    | !{item.name}
                            p(class="search-desc")
                                if(item.desc)
                                    | !{item.desc}
                                a(class="pull-right",href='/'+dj[key].model+'/'+item.url)
                                    | !{lang.goto}
                        div(class="clearfix")
                        hr