div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        if(dj[key].i && dj[key].i==1)
            - var id='arrowDownDiv'
            arrow-down
        div(class="banner-content")
            if(dj[key].name)
                h1(class="banner-header",set-animate=dj[key].nameAnimate,data-animation=dj[key].nameAnimate)
                    | !{dj[key].name}
            schedule-place(place-id=dj[key]._id,templ=dj[key].templ,mobile=mobile,data-stuff=dj[key].scheduleStuff)




