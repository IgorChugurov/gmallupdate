div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 block-map",set-animate=dj[key].animate&&!mobile,data-animation=dj[key].animate)
            div(class="container")
                div(class="col-lg-6 col-md-6 col-sm-6 col-xs-12")
