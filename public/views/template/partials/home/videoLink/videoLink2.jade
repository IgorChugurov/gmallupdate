div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 video-link")
            div(class="container")
                h2=dj[key].name
                div(class="col-lg-6 col-md-6 col-sm-12 col-xs-12 left-block")
                    | !{dj[key].desc}
                div(class="col-lg-6 col-md-6 col-sm-12 col-xs-12 right-block")
                    | !{dj[key].videoLink}
                div(class="clearfix")
        div(class="clearfix")
div(class="clearfix")