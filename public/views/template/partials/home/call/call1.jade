div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="container")
            - var className="col-lg-12 col-md-12 col-sm-12 col-xs-12 home-section full text-center"
            if(dj[key].img)
                - className  ="col-lg-6 col-md-6 col-sm-6 col-xs-12 home-section full text-center"
            div
                h3=dj[key].name
                if(dj[key].img)
                    div(class=className)
                        img(class='img-responsive',src=photoHost+'/'+dj[key].img,alt=dj[key].alt)
                div(class=className)
                    call-from-store
            div(class="clearfix")
    


