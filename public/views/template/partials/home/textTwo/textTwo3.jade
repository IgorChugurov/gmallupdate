div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        div(class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-two")
            div(class="container")
                div(class="col-lg-6 col-md-6 col-sm-6 col-xs-12 left-box")
                    h3=dj[key].name
                    hr(class="hr-top")
                    div(class="col-lg-1 col-md-1 col-sm-2 col-xs-2 img-block")
                        img(src=dj[key].img,class="img-responsive text-img",alt=dj[key].alt)
                    div(class="col-lg-11 col-md-11 col-sm-10 col-xs-10 left-text")
                        | !{dj[key].desc}
                    div(class="clearfix")
                    div(class="btn-box")
                        if(dj[key].button && dj[key].button.is)
                            a(set-animate=dj[key].button.animate,data-animation=dj[key].button.animate,class="btn btn-project btn-banner",ng-click=dj[key].button.ngClick,href=dj[key].button.href)=dj[key].button.text
                    hr(class="hr-bottom")
                div(class="col-lg-6 col-md-6 col-sm-6 col-xs-12 right-box")
                    h3=dj[key].name1
                    hr(class="hr-top")
                    div(class="col-lg-1 col-md-1 col-sm-2 col-xs-2 img-block")
                        img(src=dj[key].img,class="img-responsive text-img",alt=dj[key].alt)
                    div(class="col-lg-11 col-md-11 col-sm-10 col-xs-10 right-text")
                        | !{dj[key].desc1}

                    div(class="clearfix")
                    div(class="btn-box")
                        if(dj[key].button && dj[key].button.is)
                            a(set-animate=dj[key].button1.animate,data-animation=dj[key].button1.animate,class="btn btn-project btn-banner",ng-click=dj[key].button.ngClick,href=dj[key].button.href)=dj[key].button1.text
                    hr(class="hr-bottom")
                div(class="clearfix")
        div(class="clearfix")
div(class="clearfix")