div(class=dj[key].wrapclass)
    div(class=mobileWrapper)
        ul(class="social-links list-inline")
            if(sn.fb && sn.fb.is)
                li
                    a(href=sn.fb.link,target="_blank")
                        span Facebook

            if(sn.inst && sn.inst.is)
                li
                    a(href=sn.inst.link,target="_blank")
                        span Instagram

            if(sn.tw && sn.tw.is)
                li
                    a(href=sn.tw.link,target="_blank")
                        span Twitter

            if(sn.gl && sn.gl.is)
                li
                    a(href=sn.gl.link,target="_blank")
                        span Google

            if(sn.yt && sn.yt.is)
                li
                    a(href=sn.yt.link,target="_blank")
                        span Youtube

            if(sn.ok && sn.ok.is)
                li
                    a(href=sn.ok.link,target="_blank")
                        span OK

            if(sn.pin && sn.pin.is)
                li
                    a(href=sn.pin.link,target="_blank")
                        span Pinterest

            if(sn.vk && sn.vk.is)
                li
                    a(href=sn.vk.link,target="_blank")
                        span Vkontakte
            div(class="clearfix")


